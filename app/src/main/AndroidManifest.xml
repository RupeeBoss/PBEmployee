<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.policyboss">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".splashscreen.SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".utility.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".utility.FirebaseIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name=".navigationview.HomeActivity"
            android:label="@string/title_activity_navigation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".carinsurance.CarInsuranceActivity"
            android:label="@string/title_activity_car_insurance"
            android:parentActivityName=".navigationview.HomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.policyboss.navigationview.HomeActivity" />
        </activity>
        <activity
            android:name=".carinsurance.RenewalCarActivity"
            android:label="@string/title_activity_renewal_car"
            android:parentActivityName=".carinsurance.CarInsuranceActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.policyboss.carinsurance.CarInsuranceActivity" />
        </activity>
        <activity
            android:name=".carinsurance.CarVarientDetailsActivity"
            android:label="@string/title_activity_car_varient_details"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".carinsurance.CarAdditionalDetailsActivity"
            android:label="@string/title_activity_car_additional_details"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".carinsurance.CarDetailsActivity"
            android:label="@string/title_activity_car_details"
            android:parentActivityName=".carinsurance.CarInsuranceActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.policyboss.carinsurance.CarInsuranceActivity" />
        </activity>
        <activity
            android:name=".carinsurance.FastLaneCarDetails"
            android:label="@string/title_activity_fast_lane_car_details"
            android:parentActivityName=".carinsurance.CarInsuranceActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.policyboss.carinsurance.CarInsuranceActivity" />
        </activity>
        <activity
            android:name=".carinsurance.PopUpCardQuoteActivity"
            android:label="@string/title_activity_pop_up_card_quote"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name=".carinsurance.CarQuoteGenerate"
            android:label="@string/title_activitycard_quote"
            android:parentActivityName=".carinsurance.CarInsuranceActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.policyboss.carinsurance.CarInsuranceActivity" />
        </activity>
        <activity
            android:name=".carinsurance.PremiumPopUpActivity"
            android:label="@string/title_activity_premium_pop_up"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name=".healthinsurance.HealthInsuranceActivity"
            android:label="@string/title_activity_health_insurance"
            android:parentActivityName=".navigationview.HomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.policyboss.navigationview.HomeActivity" />
        </activity>
        <activity
            android:name=".healthinsurance.HealthInsuranceAgeDetailActivity"
            android:label="@string/title_activity_health_insurance_age_detail"
            android:parentActivityName=".healthinsurance.HealthInsuranceActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.policyboss.healthinsurance.HealthInsuranceActivity" />
        </activity>
        <activity
            android:name=".webview.WebViewBuyInsurenceActivity"
